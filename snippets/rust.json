  "10x-modules-section": {
    "prefix": "10x-modules-section-01",
    "body": [
      "// region:    --- Modules",
      "${1}",
      "use ${2:crate}::{${3}Result};",
      "${4}",
      "// endregion: --- Modules"
    ],
    "description": "Simple modules section (default `use crate::Error`)"
  },
