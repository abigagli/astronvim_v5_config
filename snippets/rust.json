  "10x-comment-section": {
    "prefix": "10x-comment-section",
    "body": [
      "// region:    --- ${1:Section}",
      "${2}$TM_SELECTED_TEXT",
      "// endregion: --- ${1:Section}"
    ],
    "description": "Comment Section"
  },
  "10x-comment-delimiter": {
